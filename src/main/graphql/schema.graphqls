type Query {
    flowClaimFooNext: Flow!
    flowClaimBarNext: Flow!
    flowClaimBazNext: Flow!
    nested: Nested
}

type Nested {
    flows: [Flow]
}


type Flow {
    id: ID!
    currentStep: FlowStep!
    context: FlowContext!
}

type FlowStep {
    id: ID!
    value: Int
}

type FlowContext {
    name: String
}